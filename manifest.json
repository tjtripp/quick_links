{
    "manifest_version": 2,
    "name": "Quick Links",
    "short_name": "QuickLinks",
    "version": "0.0.005",
    "version_name": "0.0.005",
    "description": "Provides configurable quick links.",
    "permissions": ["activeTab", "storage"],
    "icons": {
        "16": "icon16.png",
        "24": "icon24.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "100": "icon100.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icon16.png",
            "24": "icon24.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "64": "icon64.png",
            "100": "icon100.png",
            "128": "icon128.png"
        },
        "default_title": "Quick Links",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
        "scripts": ["config.js", "background.js"]
    }
    
}